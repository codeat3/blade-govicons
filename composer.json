{
    "name": "codeat3/blade-govicons",
    "description": "A package to easily make use of Gov Icons in your Laravel Blade views.",
    "keywords": ["Blade", "gov", "icons", "Laravel"],
    "homepage": "https://github.com/codeat3/blade-govicons",
    "license": "MIT",
    "authors": [
        {
            "name": "Swapnil Sarwe",
            "homepage": "https://swapnilsarwe.com"
        },
        {
            "name": "Dries Vints",
            "homepage": "https://driesvints.com"
        }
    ],
    "require": {
"php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0"    
},
    "require-dev": {
"codeat3/blade-icon-generation-helpers": "^0.3",
        "codeat3/phpcs-styles": "^1.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"    
},
    "autoload": {
        "psr-4": {
            "Codeat3\\BladeGovIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Codeat3\\BladeGovIcons\\BladeGovIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    }
}
